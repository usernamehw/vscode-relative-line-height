{
	"name": "relative-line-height",
	"displayName": "Relative Line Height",
	"description": "Use relative line height for editors.",
	"version": "1.0.1",
	"publisher": "usernamehw",
	"license": "MIT",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"relative",
		"line",
		"height"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/usernamehw/vscode-relative-line-height"
	},
	"activationEvents": [
		"*"
	],
	"main": "./dist/extension",
	"contributes": {
		"configuration": {
			"title": "Relative Line Height",
			"properties": {
				"relativeLineHeight.value": {
					"type": "number",
					"default": 0,
					"description": "Relative value of editor line height (absolute line height is computed based on editor font size and relative line height)."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production --colors",
		"compile": "webpack --mode development",
		"watch": "webpack --mode development --watch --display=minimal"
	},
	"devDependencies": {
		"@types/node": "^10.14.20",
		"@types/vscode": "^1.30.0",
		"@typescript-eslint/eslint-plugin": "^2.3.2",
		"@typescript-eslint/parser": "^2.3.2",
		"eslint": "^6.5.1",
		"friendly-errors-webpack-plugin": "^1.7.0",
		"ts-loader": "^6.2.0",
		"typescript": "^3.6.3",
		"webpack": "^4.41.0",
		"webpack-cli": "^3.3.9"
	}
}
